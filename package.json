{
  "description": "Company website",
  "version": "1.6.0",
  "repository": "https://github.com/pureblueessentialsorg/pureblueessentials.com.au",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 60",
    "Firefox ESR",
    "iOS >= 12",
    "Safari >= 12",
    "Explorer >= 11"
  ],
  "scripts": {
    "dev": "exec-bin node_modules/.bin/hugo/hugo server --disableFastRender --gc",
    "dev:preview": "exec-bin node_modules/.bin/hugo/hugo server --disableFastRender --gc --buildDrafts --buildFuture",
    "build": "exec-bin node_modules/.bin/hugo/hugo -v --gc --minify --cleanDestinationDir",
    "start": "exec-bin node_modules/.bin/hugo/hugo server --disableFastRender --renderToDisk --cleanDestinationDir",
    "start:preview": "exec-bin node_modules/.bin/hugo/hugo server --renderToDisk --cleanDestinationDir --disableFastRender --buildDrafts --buildFuture ",
    "start:production": "exec-bin node_modules/.bin/hugo/hugo server --renderToDisk --cleanDestinationDir --disableFastRender --gc --minify --environment production",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles && npm run -s lint:markdown",
    "lint:scripts": "eslint assets/js config functions",
    "lint:styles": "stylelint \"assets/scss/**/*.{css,sass,scss,sss,less}\"",
    "lint:markdown": "markdownlint *.md content/**/*.md",
    "npm-check": "npx npm-check -u",
    "hugo-version": "exec-bin node_modules/.bin/hugo/hugo version",
    "postinstall": "hugo-installer --version otherDependencies.hugo --extended --destination node_modules/.bin/hugo",
    "release": "standard-version",
    "release:dry-run": "standard-version --dry-run",
    "netlify-build": "Invoke-WebRequest -method POST -body \" \" -uri https://api.netlify.com/build_hooks/6257916d3c52761aa35a7367"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "markdownlint-cli": "^0.30.0",
    "npm-check": "^5.9.2",
    "standard-version": "^9.3.2",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0"
  },
  "otherDependencies": {
    "hugo": "0.92.2"
  },
  "dependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@popperjs/core": "^2.11.2",
    "autoprefixer": "^10.4.2",
    "bootstrap": "^5.1.3",
    "core-js": "^3.20.3",
    "exec-bin": "^1.0.0",
    "hugo-installer": "^3.1",
    "lazysizes": "^5.3.2",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "purgecss-whitelister": "^2.4",
    "quicklink": "^2.2.0",
    "regenerator-runtime": "^0.13.9",
    "shx": "^0.3.4",
    "swiper": "^7.4.1"
  }
}
